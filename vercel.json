{
    "version": 2,
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "main.py"
        }
    ],
    "env": {
        "VERCEL": "1"
    },
    "functions": {
        "main.py": {
            "maxDuration": 60
        }
    }
}